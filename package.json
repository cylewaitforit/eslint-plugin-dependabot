{
	"name": "eslint-plugin-dependabot",
	"version": "0.6.1",
	"description": "Eslint Plugin to enforce Dependabot best practices",
	"keywords": [
		"dependabot",
		"eslint",
		"eslint-plugin",
		"eslintplugin",
		"lint",
		"yaml",
		"yml"
	],
	"repository": {
		"type": "git",
		"url": "git+https://github.com/cylewaitforit/eslint-plugin-dependabot.git"
	},
	"license": "MIT",
	"author": {
		"name": "cylewaitforit",
		"email": "cyle@cylewaitfor.it"
	},
	"type": "module",
	"main": "lib/index.js",
	"files": [
		"lib/"
	],
	"scripts": {
		"build": "tsdown",
		"build:docs": "pnpm build && eslint-doc-generator && prettier --write README.md docs/rules/*.md",
		"format": "prettier .",
		"lint": "eslint . --max-warnings 0",
		"lint:docs": "pnpm build:docs && git diff --exit-code README.md docs/rules/*.md || (echo 'Documentation is out of date. Run: pnpm build:docs' && exit 1)",
		"lint:knip": "knip",
		"lint:md": "markdownlint \"**/*.md\" \".github/**/*.md\" --rules sentences-per-line",
		"lint:packages": "pnpm dedupe --check",
		"lint:spelling": "cspell \"**\" \".github/**/*\"",
		"prepare": "lefthook install",
		"should-semantic-release": "should-semantic-release --verbose",
		"test": "vitest",
		"tsc": "tsc"
	},
	"dependencies": {
		"yaml": "catalog:"
	},
	"devDependencies": {
		"@eslint-community/eslint-plugin-eslint-comments": "catalog:",
		"@eslint/js": "catalog:",
		"@release-it/conventional-changelog": "catalog:",
		"@types/eslint-plugin-markdown": "catalog:",
		"@types/node": "catalog:",
		"@vitest/coverage-v8": "catalog:",
		"@vitest/eslint-plugin": "catalog:",
		"console-fail-test": "catalog:",
		"cspell": "catalog:",
		"eslint": "catalog:",
		"eslint-doc-generator": "catalog:",
		"eslint-plugin-dependabot": "catalog:",
		"eslint-plugin-eslint-plugin": "catalog:",
		"eslint-plugin-jsdoc": "catalog:",
		"eslint-plugin-jsonc": "catalog:",
		"eslint-plugin-markdown": "catalog:",
		"eslint-plugin-n": "catalog:",
		"eslint-plugin-package-json": "catalog:",
		"eslint-plugin-perfectionist": "catalog:",
		"eslint-plugin-pnpm": "catalog:",
		"eslint-plugin-regexp": "catalog:",
		"eslint-plugin-yml": "catalog:",
		"eslint-yaml": "catalog:",
		"knip": "catalog:",
		"lefthook": "catalog:",
		"markdownlint": "catalog:",
		"markdownlint-cli": "catalog:",
		"pkg-pr-new": "catalog:",
		"prettier": "catalog:",
		"prettier-plugin-curly": "catalog:",
		"prettier-plugin-packagejson": "catalog:",
		"prettier-plugin-sh": "catalog:",
		"release-it": "catalog:",
		"sentences-per-line": "catalog:",
		"should-semantic-release": "catalog:",
		"tsdown": "catalog:",
		"typescript": "catalog:",
		"typescript-eslint": "catalog:",
		"vitest": "catalog:"
	},
	"peerDependencies": {
		"eslint": ">=9.0.0",
		"eslint-yaml": ">=0.1.0"
	},
	"packageManager": "pnpm@10.16.0+sha512.8066e7b034217b700a9a4dbb3a005061d641ba130a89915213a10b3ca4919c19c037bec8066afdc559b89635fdb806b16ea673f2468fbb28aabfa13c53e3f769",
	"engines": {
		"node": ">=22.0.0"
	},
	"publishConfig": {
		"provenance": true
	}
}
